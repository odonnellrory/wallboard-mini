{
  "manifest_version": 2,
  "name": "Wallboard-Mini",
  "version": "1.0",
  "description": "Display 8 wallboard pages in a grid layout.",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Wallboard-Mini"
  },
  "host_permissions": ["<all_urls>"]
}
